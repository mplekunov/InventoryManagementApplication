<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.jfoenix.controls.JFXTextField?>
<AnchorPane style="-fx-background-color: grey" xmlns="http://javafx.com/javafx/11"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ucf.assignments.Controllers.EditItemController"
            prefHeight="400.0" prefWidth="400.0">

    <VBox AnchorPane.topAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0">
        <children>
            <AnchorPane>
                <children>
                    <Label text="Item Name" AnchorPane.leftAnchor="20" AnchorPane.bottomAnchor="4" AnchorPane.topAnchor="12"/>
                    <JFXTextField fx:id="nameTextField" text="test" AnchorPane.bottomAnchor="4" AnchorPane.topAnchor="12" AnchorPane.rightAnchor="20"/>
                </children>
            </AnchorPane>
            <AnchorPane>
                <children>
                    <Label text="Item Price" AnchorPane.leftAnchor="20" AnchorPane.bottomAnchor="4" AnchorPane.topAnchor="12"/>
                    <JFXTextField fx:id="priceTextField" text="test" AnchorPane.bottomAnchor="4" AnchorPane.topAnchor="12" AnchorPane.rightAnchor="20"/>
                </children>
            </AnchorPane>
            <AnchorPane>
                <children>
                    <Label text="Item Serial Number" AnchorPane.leftAnchor="20" AnchorPane.bottomAnchor="4" AnchorPane.topAnchor="12"/>
                    <JFXTextField fx:id="serialNumberTextField" text="test" AnchorPane.bottomAnchor="4" AnchorPane.topAnchor="12" AnchorPane.rightAnchor="20"/>
                </children>
            </AnchorPane>
        </children>
    </VBox>
</AnchorPane>